!! Note, from this namelist, most unused options have been removed for clarity.

&INIT_GRID_DESCRIPTION
!* Direction of the grid (1 - E->W, S->N, B->U)
griddir = 1,1,1

!* Does the netcdf data start with a zero index?
!? this may just mean whether tracmass should count from 0 or 1
!? and has nothing to do with the data
zeroindx = .FALSE.

!* Describe filenames

!* directory of physical data (u,v,T,S,MLD,etc)
physDataDir = '',
physPrefixForm = '',
dateformat = 'YYYY_MM'

!* name of Tgrid, e.g. grid_T
tGridName = '_thetao',

!* name of Ugrid
uGridName = '_uo',

!* name of Vgrid
vGridName = '_vo',

!* suffix of files, e.g. ".nc"
fileSuffix = '.nc',

!* Describe variable names
!* name of resolved zonal velocity
ueul_name = 'uo',
!* name of resolved meridional velocity
veul_name = 'vo',
/

&INIT_GRID_SIZE
!* Size of global grid
!* number of global i points
imt = 117,
!* number of global j points
jmt = 116,
!* number of global k points
km = 18,

!* zonal periodic
iperio = 0
!* meridional boundary conditions
jperio = 0

!* directory of grid data
topoDataDir = '',

!* file with dz etc.
zgridFile = 'zgrid.txt'
/

&INIT_GRID_SUBDOMAIN
!* Initialise subdomain
l_subdom = .FALSE.
/

&INIT_GRID_TIME
!* time between two time levels
!* if ncgcm_unit = 5, then ngcm_step = 3 means 3 months
ngcm_step = 1,

!* units of ngcm
ngcm_unit = 4,

!* number of subcycles between time levels
iter = 48,
/

&INIT_START_DATE
!* start time
startsec = 0,
startmin = 0,
starthour = 0,

!* start date
startday = 23,
startmon = 9,
startyear = 2005,

!* don't use leap years?
noleap = .FALSE.,

mon30day = .FALSE.
/

&INIT_RUN_TIME
!* Loop run
!* make a time loop to run
loopYears = .FALSE.,

!* Verbose
log_level = 0

!* Information of run time
!* number of time steps for run
intrun = 1000
/

&INIT_WRITE_TRAJ

!* write_frec = 1 - write at time intervals of gcm datasets (each ints)
!*            = 2 - write at each time iteration
!*            = 3 - write each spatial grid-crossing
!*            = 4 - write all the time steps
!*            = 5 - write only start and end positions
!*            = 6 - write (1) at time intervals of gcm datasets and (4) each spatial grid-crossing
write_frec = 6

outDataDir = '',
outDataFile = '',

!* Format of the time array
!* timeformat = 0 - tt
!*            = 1 - ts
!*            = 2 - YYYY, MM, DD, HH
timeformat = 1
/

&INIT_SEEDING
!* start only one trajectory for debugging
loneparticle = 0,

!* nff = 1 run forward trajectories
!*     = -1 run backwards
nff = -1,

!* Number of trajectories can be set by
!* nqua = 1 constant number of particles in all seeding cells
!*          set by partQuant (particles / gridcell)
!*      = 2 Each particle reflects mass transport at seeding.
!*          set by partQuant (m3/s or kg/s per particle)
!*      = 3 Each particle reflects air/water mass/volume at seeding.
!*          set by partQuant in m3 or kg per particle
nqua = 2,

!* particles/gridcell or m3s-1/particle or m3/particle
partquant = 200,

!* seedtype = 1 seed using ist,jst,kst (below)
!*          = 2 seed using file with start positions
seedtype = 2,

!! when seedtype = 2

!* directory for seed files
seeddir = '',

!* name of seed file
seedfile = 'seeds.txt',

!* seedtime = 1 seed only for specific times
!*          = 2 seed using file with start positions
seedtime =  1,

!! when seedtime = 1

!* seed only between tst1 and tst2
tst1 = 1,
tst2 = 10,
/

&INIT_TRACERS
!* Should we evaluate tracers along the trajectories?
l_tracers = .FALSE.
/

&INIT_TRACERS_SEEDING
/

&INIT_KILLZONES
!* time limit for traj [days]
timax = 100000

exitType = 1
!* If exitType = 1: killing zone defined by a box
!* IMPORTANT -> The maximum number of boxes than can be defined are 10
!* ienw,iene are western and eastern i indices of killzone
!* jens,jenn are southern and northern j indices of killzone
ienw = 0, 0, 2, 116, 19
iene = 118, 118, 2, 116, 20
jens = 2, 115, 0, 0, 97
jenn = 2, 115, 117, 117, 98
/

&INIT_POSTPROCESS
!* Compute stream Functions
l_psi = .FALSE.
/

&INIT_ACTIVE
/
